#include<stdio.h>
#include<string.h>
void main()
{
	char str[10];
	gets(str);
	int i,count=0,count1=0,count2=0;
	for(i=0;str[i]!='\0';i++)
	{
		if(isupper(str[i]))
     	{
     		count=count+1;
		 }
    }
    printf("\ncount of uppercase characters:");
    printf("%d",count);
{
for(i=0;str[i]!='\0';i++)
	{
		if(islower(str[i]))
     	{
     		count1=count1+1;
		 }
    }
    printf("\ncount of lowercase characters:");
    printf("%d",count1);
}

for(i=0;str[i]!='\0';i++)
	{
		if(isdigit(str[i]))
     	{
     		count2=count2+1;
		 }
    }
    printf("\ncount of numbers:");
    printf("%d",count1);

}
